<launch>
    <include file="$(find zhongqi_navigation)/launch/amcl.launch" />
    
    <node args="/home/autolabor/map/mp1.yaml" name="map_server" output="screen" pkg="map_server" type="map_server">
    </node>
     
	<node name="move_base" output="screen" pkg="move_base" respawn="false" type="move_base">
	    <rosparam command="load" file="$(find zhongqi_navigation)/param/move_base_mpc_params.yaml" />
  	  	<rosparam command="load" file="$(find zhongqi_navigation)/param/costmap_common_params.yaml" ns="global_costmap" />
  	 	<rosparam command="load" file="$(find zhongqi_navigation)/param/costmap_common_params.yaml" ns="local_costmap" />
  		<rosparam command="load" file="$(find zhongqi_navigation)/param/local_costmap_params.yaml" />
  		<rosparam command="load" file="$(find zhongqi_navigation)/param/global_costmap_params.yaml" />
        <rosparam command="load" file="$(find zhongqi_navigation)/param/global_planner_params.yaml" ns="GlobalPlanner" />    
  		
        <rosparam command="load" file="$(find zhongqi_navigation)/param/mpc_local_planner_params.yaml" />
        <!--rosparam command="load" file="$(find zhongqi_navigation)/param/teb_local_planner_params.yaml" /-->
        <!--remap from="/cmd_vel" to="/mb_cmd"/-->

	</node>
	  
    <node args="-d $(find zhongqi_navigation)/rviz/teb_navigation.rviz" name="rviz" pkg="rviz" type="rviz" />
    <!--node name="pure_pursuit" pkg="zhongqi_driver" type="pure_pursuit.py"  output="screen" /-->


</launch>
